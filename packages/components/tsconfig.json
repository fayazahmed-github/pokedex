{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
      "outDir": "dist",
      "jsx": "react-jsx",
      "declaration": true,
      "declarationDir": "dist/types",
      "isolatedModules": true,
      "lib": ["dom", "dom.iterable", "esnext"]
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"],
    "path": {
        "mui": ["./node_modules/@mui"]
    },
    "moduleResolution": "node"
  }